{
  "name": "codingmaxima-courses",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve -o --port 2260",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "optimize-build": "ng build && gzipper c ./dist --include js,css,html --output-file-format [filename].[ext] ./optimized-build/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
