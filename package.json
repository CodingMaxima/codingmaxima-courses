    "@angular/platform-browser": "^15.2.5",
    "@angular/platform-browser-dynamic": "^15.2.5",
    "@angular/router": "^15.2.5",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngstack/code-editor": "^3.1.0",
    "@types/jquery": "^3.5.11",
    "aws-sdk": "^2.1348.0",
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
